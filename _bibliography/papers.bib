@article{MePrPe22,
  abbr={DPR_BM25},
  bibtex_show={true},
  title={Evaluierung des Dense Passage Retrievals zur Dokumentensuche in Behörden im Vergleich zu BM25},
  author={Mehra, Anket and Prieß, Malte and Peters, Tom V.},
  journal={(submitted) <br> Jahrestagung des Arbeitskreises Wirtschaftsinformatik an Hochschulen für Angewandte Wissenschaften},
  year={2022},
  abstract={Abstract: Die Suche nach Dokumenten ist eine der Aufgaben, die Maschinen am häufigsten ausführen. Täglich genutzte Suchmaschinen suchen nach Dokumenten, um das Informationsbedürfnis eines Anfragenden zu erfüllen. Aktuelle Forschungsergebnisse zeigen, dass der Bewertungsansatz des Dense Passage Retrieval (DPR) – welcher die zu suchenden Dokumente mittels tiefer neuronaler Netze in Dense-Vektoren umwandelt – in englischer ebenso wie in deutscher Sprache bessere Ergebnisse liefert als aktuell implementierte Algorithmen wie der BM25-Algorithmus. Die Dokumentensuche in deutschen Behörden stellt eine besondere Aufgabe dar, da in diesen komplexe behördenspezifische Fachausdrücke verwendet werden. Im Rahmen dieser Arbeit wurde für die Dokumentensuche zu Corona-Regelungen in der öffentlichen Verwaltung ein DPR mit einem öffentlichen deutschsprachigen Datensatz trainiert und mit dem BM25-Algorithmus von Elasticsearch anhand der Relevanz als Gütemaß verglichen. Die Ergebnisse zeigen, dass das DPR mehr Dokumente mit gesuchten Informationen findet, Synonyme in Suchanfragen berücksichtigen kann und damit das Potenzial zur Verbesserung der Dokumentensuche in Behördenanwendungen hat. <br><br> 
  Keywords: Dense Passage Retrieval, BM25, Natürliche Sprachverarbeitung, Künstliche Intelligenz, Neuronale Netze, Information Retrieval, Dokumentensuche},
  selected={true}
}

@article{JaPrKr22,
  abbr={KIBank},
  bibtex_show={true},
  title={Entwicklung einer KI für die Zahlungskorrektur im Banken-Zahlungsverkehr},
  author={Janz, Katharina and Prieß, Malte and Kraas, Helge},
  journal={(submitted) <br> Jahrestagung des Arbeitskreises Wirtschaftsinformatik an Hochschulen für Angewandte Wissenschaften},
  year={2022},
  abstract={Abstract: Beschrieben wird die Entwicklung eines Prototyps für ein Machine Learning-System zur Korrektur von fehlerhaften Zahlungsdaten, der für die Anwendung TRAVIC-Payment Hub der PPI AG entwickelt wurde. Hierfür stehen künstlich generierte Testdaten mit verschiedenen Korrekturen zur Verfügung, anhand derer eine Klassifikation erfolgt. Die Daten bilden ein unausgewogenes Klassenverhältnis für ausschließlich kategoriale Merkmal ab. Diese können teilweise sehr hohe Kardinalitäten annehmen. Für die Erstellung der Machine Learning-Modelle zur Vorhersage der Korrekturen werden die Klassifikatoren XGBoost und Naive Bayes verwendet. Für XGBoost werden zusätzlich die ordinale und One-Hot-Kodierung sowie das Merkmals-Hashing miteinander verglichen. Die jeweils ausgegebenen Vorhersagen sollen anhand der vom Modell errechneten Wahrscheinlichkeit beurteilt werden können. Fällt diese unter einen gewissen Schwellwert, soll dies als Indiz für eine falsche Vorhersage gewertet werden können. Insgesamt sind die XGBoost-Modelle besser geeignet als Naive Bayes. <br><br>
  Keywords: Künstliche Intelligenz, Machine Learning, Klassifikation, Entscheidungsbäume, XGBoost, Naives Bayes, Merkmals-Kodierung},
  selected={true}
}

@article{JacPri22,
  abbr={ProcMin},
  bibtex_show={true},
  title={Process Mining – Eine anwendungsfallbasierte Einführung anhand des Purchase-to-Pay Prozesses},
  author={Jacobsen, Michel and Prieß, Malte},
  journal={(submitted) <br> Jahrestagung des Arbeitskreises Wirtschaftsinformatik an Hochschulen für Angewandte Wissenschaften},
  year={2022},
  abstract={Abstract: Process Mining als junge Disziplin des Geschäftsprozessmanagements ermöglicht die Generierung von Wissen aus Ereignisdaten. Diese Daten entstehen in Systemen, welche die Durchführung von Arbeits- bzw. Prozessschritten dokumentieren. Die Bereitstellung und Extraktion der Daten aus den entsprechenden Softwaresystemen soll in dieser Ausarbeitung anhand des Purchase-to-Pay-Prozesses der Queisser Pharma GmbH & Co. KG dargestellt werden. Der Anwendungsfall zeigt zudem, wie ein Unternehmen selbstständig Erkenntnisse und Know-How im Bereich des Process-Minings aufbauen kann und welchen Mehrwert die Auswertung der Ereignisdaten bietet. Die Datenvorbereitung und -Verarbeitung wird mit Hilfe frei zugänglicher Programmiersprachen (Python) umgesetzt. Eine Eigenentwicklung wird notwendig, wenn kommerzielle Tools einen Prozess nicht abbilden oder nur durch teure Entwicklungen ermöglichen. Notwendigkeit hierfür besteht zudem bei ERP-Systemen, die vom Unternehmen selbstständig entwickelt wurden – eine Verwendung kommerzieller Tools ist damit fast ausgeschlossen. Der Anwendungsfall zeigt, welchen Mehrwert die Datenvorbereitung für Process Mining Verfahren mit Hilfe von frei zugänglichen Tools für Unternehmen bieten kann. <br><br>
  Keywords: Process Mining, Geschäftsprozessmanagement, Anwendungsfall, Datenerhebung, Datenvorbereitung, SAP, Purchase-to-Pay-Prozess},
  selected={true}
}